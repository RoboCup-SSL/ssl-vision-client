{
  "name": "ssl-vision-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "eslint": "5",
    "eslint-plugin-vue": "5",
    "vue": "2",
    "vue-native-websocket": "2",
    "vuex": "3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3",
    "@vue/cli-plugin-eslint": "3",
    "@vue/cli-service": "3",
    "@vue/eslint-config-prettier": "3",
    "babel-eslint": "10",
    "vue-template-compiler": "2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
